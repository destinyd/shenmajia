<div class="bill" data-time="<%= bill.created_at.to_i %>" id="bill_<%= bill.id %>">  
  <div class="l">
    <%=link_to  bill,bill,target: '_blank', title: bill %><br />
    账单总额：<%= bill.total %><br />
    <% if bill.locatable %>
      地点：<%= link_to bill.locatable, bill.locatable %>
    <% end %>
  </div>  
  <div class="images">
    <% bill.bill_prices.each do |bp| %>
      <%= link_to bp.price.good,title: bp.price.human_name do %>
        <%= upload_image_tag bp.image,bp.price.human_name,:list %>
      <% end %>
    <% end -%>
  </div>
</div>

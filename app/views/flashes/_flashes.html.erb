<%= content_tag :ul do %>
  <% @flashes.each do |flash| %>
    <%= content_tag :li do %>
      <% if flash.outlink %>
        <%= link_to flash.outlink.url do%>
          <%= image_tag(flash.image.url(:flash_pic)) -%>
        <% end -%>
      <% else %>
        <%= image_tag(flash.image.url(:flash_pic)) -%>
      <% end %>
    <% end -%>
  <% end -%>
<%end -%>

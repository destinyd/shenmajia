<div class="price c" data-time="<%= price.created_at.to_i %>">  
  <div class="l">
    <%= link_to price do %>
      <%= image_tag price.image.url(:list) %>
    <% end %>
  </div>
  <div class="detail">[<%= price.type_id %>] <%=link_to  price.name.try(:block,150),price,target: '_blank' %>
    <br />
    价格：<%= link_to price,price %>
    <% if price.address %><br />地址：<%= price.address %><% end %>
    <% if price.finish_at %><br />
      截止日期：<em> <%= price.finish_at %></em><% end -%>
  </div>
</div>

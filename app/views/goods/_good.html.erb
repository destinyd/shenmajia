<%= content_tag :ul,:class => 'table' do %>
  <%= content_tag 'li',:class => 'name' do -%>
    <%= link_to good,good,:title => good.name %>
  <% end %>
  <%= content_tag 'li' do -%>
    <%= good.norm %>
  <% end %>
  <%= content_tag 'li' do -%>
    <%= good.unit %>
  <% end %>
  <%= content_tag 'li' do -%>
    <%= good.origin %>
  <% end %>
<% end %>
